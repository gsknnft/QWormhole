{
  "name": "@qwormhole/console",
  "version": "1.0.0",
  "description": "Real-time visualization of the Negentropic Coupling Framework - Quantum Electron Edition",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Gordon Skinner",
    "email": "gsknnft@gmail.com",
    "url": "https://github.com/gsknnft"
  },
  "main": "app/dist/main/index.js",
  "workspaces": [
    "../packages/*"
  ],
  "scripts": {
    "dev": "vite",
    "dev:console": "concurrently \"pnpm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:dev2": "wait-on http://localhost:5173 && electron .",
    "build:tsc": "tsc && vite build && tsc -p tsconfig.main.json && tsc -p tsconfig.preload.json",
    "build:console": "pnpm run build && electron-builder",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "link:electron-react": "pnpm run ./scripts/linkConfigs",
    "build": "ts-node scripts/build.ts",
    "build:main": "vite build --config configs/vite.main.config.ts",
    "build:preload": "vite build --config configs/vite.preload.config.ts",
    "build:renderer": "vite build --config configs/vite.renderer.config.ts",
    "electron:dev": "wait-on tcp:5173 && electron .",
    "build:pub": "vite build && electron-updater build --publish",
    "postinstall:old": "cross-env CI=true ts-node scripts/ensure-package-manager.ts && ts-node scripts/update-electron-version.ts && ts-node scripts/check-native-dep.ts && pnpm run postinstall:build",
    "postinstall": "cross-env CI=true ts-node scripts/postinstall.ts",
    "package": "ts-node ./scripts/clean.ts dist && vite build && electron-updater build --publish never",
    "package:wp": "pnpm run build && electron-updater build --publish never",
    "build:conc": "concurrently \"pnpm run build:main\" \"pnpm run build:renderer\"",
    "dist:clean": "pnpm dlx rimraf app/dist",
    "lint": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "package:og": "ts-node ./scripts/clean.ts dist && pnpm run build && electron-updater build --publish never && pnpm run build:pub",
    "prestart": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true vite ./src/main/index.ts",
    "start": "ts-node ./scripts/check-port-in-use.ts && pnpm prestart && pnpm start:renderer",
    "start:main": "concurrently -k \"cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true vite --watch --config ./src/main/index.ts\" \"electronmon .\"",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true vite --config electron.vite.config.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true vite",
    "doctor:native": "ts-node scripts/check-native-dep.ts",
    "test": "jest"
  },
  "browserslist": [
    "extends browserslist-config-erb"
  ],
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "./src/jest.setup.ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "setupFiles": [
      "./scripts/check-build-exists.ts"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testPathIgnorePatterns": [
      "app/dist"
    ],
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@electron/notarize": "^3.1.1",
    "@qwormhole/console": "link:../console",
    "@gsknnft/bigint-buffer": "link:../../../../bigint-buffer",
    "@octokit/request-error": "^7.1.0",
    "@sigilnet/fft-legacy": "workspace:*",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "assert": "^2.1.0",
    "chalk": "^5.6.2",
    "crypto-browserify": "^3.12.1",
    "d3": "^7.8.5",
    "d3-force": "^3.0.0",
    "electron-debug": "^4.1.0",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "express": "5.1.0",
    "i18next": "^25.6.0",
    "i18next-electron-fs-backend": "^3.0.3",
    "i18next-fs-backend": "^2.6.0",
    "pino": "^10.1.0",
    "prettier": "^3.5.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router": "7.9.6",
    "react-router-dom": "7.9.6",
    "recharts": "^2.10.3",
    "secure-electron-context-menu": "^1.3.3",
    "secure-electron-license-keys": "^1.1.3",
    "secure-electron-store": "^4.0.3",
    "snowflake-sdk": "^2.3.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "vite": "^7.1.9",
    "yaml": "^2.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@types/d3": "^7.4.3",
    "@types/d3-force": "^3.0.9",
    "@types/express": "5.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "@types/react-test-renderer": "^19.1.0",
    "@types/source-map-support": "^0.5.10",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@vitejs/plugin-react": "^5.1.1",
    "browserslist-config-erb": "^0.0.3",
    "buffer": "^6.0.3",
    "concurrently": "^9.2.1",
    "core-js": "^3.47.0",
    "cpy-cli": "^6.0.0",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "detect-port": "^2.1.0",
    "electron": "39.2.6",
    "electron-devtools-installer": "^4.0.0",
    "electron-updater": "6.6.2",
    "electronmon": "^2.0.4",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-erb": "^4.1.0-0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-jest": "^29.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-promise": "^7.2.1",
    "express": "^5.1.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "fs-extra": "^11.3.2",
    "html-webpack-plugin": "^5.6.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "js-yaml": "^4.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "8.3.0",
    "process": "^0.11.10",
    "react-refresh": "^0.18.0",
    "react-test-renderer": "^19.0.0",
    "rimraf": "^6.1.0",
    "sass": "^1.94.1",
    "sass-loader": "^16.0.6",
    "source-map-loader": "^5.0.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.4",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "5.9.3",
    "url-loader": "^4.1.1",
    "util": "^0.12.5",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vm-browserify": "^1.1.2",
    "wait-on": "^9.0.3",
    "webpack": "^5.103.0",
    "webpack-bundle-analyzer": "^5.0.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1",
    "yaml": "^2.3.4"
  },
  "build": {
    "productName": "QuantumElectron",
    "appId": "ncf.gsknnft.QuantumElectron",
    "asar": true,
    "asarUnpack": [
      "dist/build/Release/*.node"
    ],
    "extraResources": [
      {
        "from": "dist/build/Release/",
        "to": "build/Release",
        "filter": [
          "*.node"
        ]
      },
      "./assets/**"
    ],
    "files": [
      "dist/**",
      "node_modules/**",
      "package.json"
    ],
    "electronVersion": "39.2.6",
    "npmRebuild": false,
    "buildDependenciesFromSource": false,
    "nodeGypRebuild": false,
    "electronBuilderEnv": {
      "ELECTRON_BUILDER_NODE_INSTALLER": "pnpm",
      "ELECTRON_BUILDER_PREFER_GLOBAL": "true"
    },
    "mac": {
      "sign": "scripts/notarize.js",
      "notarize": false,
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "publish": {
      "provider": "github",
      "owner": "gsknnft",
      "repo": "quantum-electron"
    }
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=24.9.0",
      "onFail": "error"
    },
    "packageManager": {
      "name": "pnpm",
      "version": "10.23.0",
      "onFail": "error"
    }
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/main/**"
    ],
    "logLevel": "quiet"
  }
}